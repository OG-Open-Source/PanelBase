{
  "plugins": {
    "hello_plugin": {
      "name": "Hello Plugin",
      "authors": "PanelBase Team",
      "version": "1.0.0",
      "description": "示例插件",
      "source_link": "https://github.com/OG-Open-Source/PanelBase/pkg/plugins/hello_plugin",
      "api_version": "v1",
      "endpoints": [
        {
          "path": "/hello",
          "methods": ["GET", "POST"],
          "input_format": {
            "name": "string",
            "age": "number"
          },
          "output_format": {
            "message": "string",
            "timestamp": "string"
          }
        },
        {
          "path": "/data",
          "methods": ["GET"],
          "input_format": {},
          "output_format": {
            "data": "array",
            "count": "number"
          }
        }
      ]
    },
    "user_manager": {
      "name": "用户管理插件",
      "authors": "PanelBase Team",
      "version": "1.0.0",
      "description": "用户管理和权限控制",
      "source_link": "https://github.com/OG-Open-Source/PanelBase/pkg/plugins/user_manager",
      "api_version": "v1",
      "endpoints": [
        {
          "path": "/users",
          "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"],
          "input_format": {
            "id": "string",
            "username": "string",
            "email": "string",
            "role": "string"
          },
          "output_format": {
            "success": "boolean",
            "data": "object",
            "message": "string"
          }
        }
      ]
    }
  }
}
